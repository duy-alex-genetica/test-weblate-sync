  "modalError": {
    "header": {
      "title": "出了些问题"
    },
    "body": {
      "message": {
        "notify": "您的更改尚未保存。",
        "contact": "如果您需要任何进一步的支持，请通过电子邮件 <1>cs@genetica.asia</1> 或热线 <3>1900 599 927</3> 与我们联系。"
      },
      "button": {
        "label": "再试一次"
      }
    }
